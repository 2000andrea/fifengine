LINKOBJ  = frm.o frmconv.o out_png.o $(RES)
LIBS =  -L"lib" -lws2_32 -lpng  
INCS =  -I"include" 
CXXINCS =  -I"lib/gcc/mingw32/3.4.2/include"  -I"include/c++/3.4.2/backward"  -I"include/c++/3.4.2/mingw32"  -I"include/c++/3.4.2"  -I"include"  -I"C:/boost_1_33_0" 
BIN  = frmconv.exe
