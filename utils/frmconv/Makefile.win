.PHONY: all all-before all-after clean clean-custom

all: all-before frmconv.exe all-after


clean: clean-custom
	${RM} $(OBJ) $(BIN)

$(BIN): $(OBJ)
	$(CC) $(LINKOBJ) -o "frmconv.exe" $(LIBS)

frm.o: frm.c
	$(CC) -c frm.c -o frm.o $(CFLAGS)

frmconv.o: frmconv.c
