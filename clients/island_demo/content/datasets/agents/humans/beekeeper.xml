<?xml version="1.0"?>
<!-- Note, this file needs to be updated to collect all the animations in one object (with varying 'direction' value) -->
<dataset id="Beekeeper" type="Embedded" format="1.0">
	<object id="Beekeeper" blocking="1">
		<action id="default">
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_rotating.xml"/>
		</action>

		<action id="talk">
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_0.xml" direction='0' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_45.xml" direction='45' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_90.xml" direction='90' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_135.xml" direction= '135' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_180.xml" direction = '180' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_225.xml" direction='225' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_270.xml" direction='270' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talking_315.xml" direction='315' />
		</action>
	</object>
</dataset>
