<?xml version="1.0"?>
<!-- Note, this file needs to be updated to collect all the animations in one object (with varying 'direction' value) -->
<dataset id="Beekeeper" type="Embedded" format="1.0">
	<object id="Beekeeper">
		<action id="default">
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_rotate.xml"/>
		</action>

		<action id="walk">
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_n.xml" direction='90' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_ne.xml" direction='45' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_e.xml" direction='0' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_se.xml" direction='315' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_s.xml" direction='270' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_sw.xml" direction='225' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_w.xml" direction = '180' />
			<animation source="content/animations/agents/humans/beekeeper/beekeeper_talk_nw.xml" direction= '135' />
		</action>
	</object>
</dataset>
