<Window title="Load Map">
	<Label text="Properties:"/>
	<ScrollArea size="200,200">
		<ListBox name="propertyList" />
	</ScrollArea>
	<HBox>
		<Label text="Name:" />
		<Spacer />
		<Label text="..." min_size="100,0"/>
	</HBox>
	<HBox>
		<Label text="Value:" />
		<Spacer />
		<TextField min_size="100,0"/>
	</HBox>
	<HBox padding="10">
		<Button name="addProperty" text="ADD" />
		<Button name="delProperty" text="REMOVE" />
		<Spacer />
		<Button name="okButton" text="Ok"/>
		<Button name="closeButton" text="Cancel"/>
	</HBox>
</Window>